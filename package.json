{
  "name": "@immowelt/babel-preset-immowelt-node",
  "version": "0.0.0-development",
  "private": false,
  "author": "Immonet dev team <aegaeon@immonet.de> (https://immonet.de)",
  "license": "MIT",
  "description": "Default shareable config of babel for projects at Immowelt.",
  "main": "src/index.js",
  "scripts": {
    "test": "yarn run lint && yarn run jest:coverage",
    "lint": "eslint src",
    "prettier": "prettier --write src/**/*.js",
    "jest": "jest",
    "jest:watch": "yarn run jest -- --watch",
    "jest:coverage": "yarn run jest -- --coverage",
    "semantic-release": "semantic-release pre && npm publish --access=public && semantic-release post",
    "precommit": "lint-staged && npm test"
  },
  "lint-staged": {
    "src/**/*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "6.26.0"
  },
  "devDependencies": {
    "@immowelt/eslint-config-immowelt-react": "2.0.4",
    "@immowelt/jest-preset-node": "1.0.1",
    "@immowelt/styleguide-javascript": "1.0.11",
    "@inkdpixels/commit-analyzer": "1.0.1",
    "@inkdpixels/release-notes-generator": "1.0.1",
    "eslint": "4.18.1",
    "babel-core": "6.26.0",
    "husky": "0.14.3",
    "jest": "22.4.2",
    "lint-staged": "6.1.1",
    "prettier": "1.11.1",
    "semantic-release": "^8.0.0"
  },
  "engines": {
    "node": ">=8.2.0"
  },
  "release": {
    "analyzeCommits": "@inkdpixels/commit-analyzer",
    "generateNotes": "@inkdpixels/release-notes-generator"
  },
  "jest": {
    "preset": "@immowelt/jest-preset-node"
  },
  "keywords": [
    "babel",
    "immowelt",
    "preset",
    "immonet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ImmoweltGroup/babel-preset-immowelt-node.git"
  }
}
